@model WebAtividadeEntrevista.Models.BeneficiarioModel


<div class="modal-header">
    <h5 class="modal-title">Tela de Beneficiário</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>


@using (Html.BeginForm())
{
    <div class="modal-body">
        @Html.AntiForgeryToken()

        <div class="form-row">
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            @Html.Hidden("ClienteId", (int?)ViewBag.ClienteId)

            @Html.Partial("_Beneficiario")

            <div class="modal-footer">
                <input type="submit" value="Adicionar" class="btn btn-success" />
                <input type="button" class="btn btn-default" data-dismiss="modal" value="Fechar" />
            </div>
        </div>
    </div>
}